{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/Home/index.html" },
    { "source": "/blog", "destination": "/blog/blog.html" },
    { "source": "/blog/:path*", "destination": "/blog/:path*" },
    { "source": "/:path*", "destination": "/Home/:path*" }
  ]
}

  